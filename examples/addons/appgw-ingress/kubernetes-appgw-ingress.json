{
  "apiVersion": "vlabs",
  "properties": {
    "orchestratorProfile": {
      "orchestratorType": "Kubernetes",
      "orchestratorRelease": "1.10",
      "kubernetesConfig": {
        "addons": [
          {
            "name": "appgw-ingress",
            "enabled": true,
            "config": {
              "appgw-sku": "WAF_v2",
              "appgw-autoscale": true,
              "watch-namespace": "ns1,ns2"
            }
          }
        ]
      }
    },
    "masterProfile": {
      "count": 1,
      "vmSize": "Standard_DS2_v2",
      "DNSPrefix": ""
    },
    "agentPoolProfiles": [
      {
        "name": "agentpool",
        "count": 1,
        "vmSize": "Standard_DS2_v2",
        "availabilityProfile": "VirtualMachineScaleSets",
        "storageProfile": "ManagedDisks"
      }
    ],
    "linuxProfile": {
      "adminUsername": "azureuser",
      "ssh": {
        "publicKeys": [
          {
            "keyData": ""
          }
        ]
      }
    }
  }
}
